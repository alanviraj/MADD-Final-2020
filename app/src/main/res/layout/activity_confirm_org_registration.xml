<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintEnd_toEndOf="parent"

    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:context=".ConfirmOrgRegistration">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="0dp"
        android:layout_height="0dp"

        android:layout_marginBottom="106dp"
        android:background="#143567"
        android:longClickable="false"
        android:theme="@style/ThemeOverlay.AppCompat.Light"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:title="Vehicle Bath"
        app:titleMarginStart="40dp"
        app:titleTextColor="#FFFFFF" />
<!--    app:logo="@drawable/logop"-->

    <Button
        android:id="@+id/btn_create_OA"
        android:layout_width="251dp"
        android:layout_height="0dp"
        android:layout_marginBottom="30dp"
        android:backgroundTint="#225EBB"
        android:fontFamily="@font/trykker"
        android:text="@string/CAbutton"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chkr2" />

    <CheckBox
        android:id="@+id/chkr2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="17dp"
        android:fontFamily="@font/trykker"
        android:text="@string/UOchk"
        app:layout_constraintBottom_toTopOf="@+id/btn_create_OA"
        app:layout_constraintEnd_toEndOf="@+id/btn_create_OA"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="41dp"
        android:fontFamily="@font/trykker"
        android:text="@string/UOname"
        android:textColor="#251B1B"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/org_con_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="27dp"
        android:fontFamily="@font/trykker"
        android:text="@string/UOemail"
        android:textColor="#251B1B"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/org_con_number"
        app:layout_constraintEnd_toStartOf="@+id/org_con_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="50dp"
        android:fontFamily="@font/trykker"
        android:text="@string/UOnumber"
        android:textColor="#251B1B"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/org_con_number"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="6dp"
        android:fontFamily="@font/trykker"
        android:text="@string/nslots"
        android:textColor="#251B1B"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/org_con_slots"
        app:layout_constraintEnd_toStartOf="@+id/org_con_slots"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="38dp"
        android:fontFamily="@font/trykker"
        android:text="@string/osubs"
        android:textColor="#251B1B"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/chkr2"
        app:layout_constraintEnd_toStartOf="@+id/org_con_subs"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/org_con_slots" />

    <EditText
        android:id="@+id/org_con_name"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:layout_marginTop="137dp"
        android:layout_marginEnd="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/org_con_subs"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="80dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/btn_create_OA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="@+id/org_con_slots" />

    <EditText
        android:id="@+id/org_con_number"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="26dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/org_con_slots"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/org_con_slots"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="45dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/org_con_number" />

    <EditText
        android:id="@+id/org_con_email"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="63dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/org_con_number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/org_con_name" />


</androidx.constraintlayout.widget.ConstraintLayout>