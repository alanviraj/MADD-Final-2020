<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".customer_blacklist"

    >


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:background="#143567"
        android:theme="@style/ThemeOverlay.AppCompat.Light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:logo="@drawable/logo13"
        app:title="@string/appName"

        app:titleMarginStart="40dp"
        app:titleTextColor="#FFFFFF" />

    <TextView
        android:id="@+id/txVwRepot1"
        android:layout_width="262dp"
        android:layout_height="43dp"
        android:layout_marginBottom="16dp"
        android:text="@string/cusBlack"
        android:textColor="#050505"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/txVwRepot2"
        android:layout_width="363dp"
        android:layout_height="116dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rectangle"
        android:text="@string/blkName"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txVwRepot1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txVwRepot3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/blkNameNum"
        android:textAlignment="viewStart"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/txVwRepot2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txVwRepot2"
        app:layout_constraintVertical_bias="0.464" />

    <Button
        android:id="@+id/btnBlack1"
        android:layout_width="151dp"
        android:layout_height="37dp"
        android:background="@drawable/btn_rectangle"
        android:paddingLeft="1dp"
        android:text="@string/activate"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txVwRepot2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txVwRepot2"
        app:layout_constraintVertical_bias="0.886" />


    <TextView
        android:id="@+id/txVwRepot4"
        android:layout_width="363dp"
        android:layout_height="116dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rectangle"
        android:text="@string/blkName1"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txVwRepot2"
        app:layout_constraintVertical_bias="0.079" />

    <TextView
        android:id="@+id/txVwRepot7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/blkNameNum1"
        android:textAlignment="viewStart"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/txVwRepot4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txVwRepot4"
        app:layout_constraintVertical_bias="0.476" />

    <Button
        android:id="@+id/btnBlack2"
        android:layout_width="151dp"
        android:layout_height="37dp"
        android:background="@drawable/btn_rectangle"
        android:paddingLeft="1dp"
        android:text="@string/activate"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txVwRepot4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txVwRepot4"
        app:layout_constraintVertical_bias="0.911" />


    <TextView
        android:id="@+id/txVwblack6"
        android:layout_width="363dp"
        android:layout_height="116dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/rectangle"
        android:text="@string/blkName2"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txVwRepot4"
        app:layout_constraintVertical_bias="0.232" />

    <TextView
        android:id="@+id/txVwblack7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/blkNameNum1"
        android:textAlignment="viewStart"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/txVwblack6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txVwblack6"
        app:layout_constraintVertical_bias="0.452" />

    <Button
        android:id="@+id/btnBlack3"
        android:layout_width="151dp"
        android:layout_height="37dp"
        android:background="@drawable/btn_rectangle"
        android:paddingLeft="1dp"
        android:text="@string/activate"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txVwblack6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txVwblack6"
        app:layout_constraintVertical_bias="0.873" />


</androidx.constraintlayout.widget.ConstraintLayout>